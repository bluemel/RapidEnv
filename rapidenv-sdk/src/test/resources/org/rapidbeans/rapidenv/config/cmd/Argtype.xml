<enumtype name="org.rapidbeans.rapidenv.config.cmd.Argtype">
	<description>Forseen argument types.</description>
	<enum name="string">
		<description>
			A string argument. Usually the default for argument choices.
		</description>
	</enum>
	<enum name="bool">
		<description>A boolean argument.</description>
	</enum>
	<enum name="date">
		<description>A date argument.</description>
	</enum>
	<enum name="file">
		<description>A file argument.</description>
	</enum>
	<enum name="integer">
		<description>An integer number argument.</description>
	</enum>
	<enum name="floating">
		<description>A floating point number argument.</description>
	</enum>
</enumtype>
