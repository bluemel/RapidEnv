<enumtype name="org.rapidbeans.rapidenv.config.Packaging">
	<header>
		<column name="extension" type="String"/>
	</header>
	<enum name="zip">
		<description>zip file.</description>
		<cell name="extension" value="zip"/>
	</enum>
	<enum name="jar">
		<description>jar file.</description>
		<cell name="extension" value="jar"/>
	</enum>
	<enum name="tar">
		<description>tar file.</description>
		<cell name="extension" value="jar"/>
	</enum>
	<enum name="ziptar">
		<description>zipped tar file.</description>
		<cell name="extension" value="tgz"/>
	</enum>
</enumtype>
