<enumtype name="org.rapidbeans.rapidenv.config.PropertyFileType">
	<description>
		Specifies if a file is a foler / directory or a simple flat file.
	</description>
	<enum name="file">
		<description>
			A flat file.
		</description>
	</enum>
	<enum name="directory">
		<description>
			A folder.
		</description>
	</enum>
</enumtype>
