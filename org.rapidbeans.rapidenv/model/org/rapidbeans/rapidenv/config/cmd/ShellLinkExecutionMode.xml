<enumtype name="org.rapidbeans.rapidenv.config.cmd.ShellLinkExecutionMode">
	<enum name="simple">
		<description>
			Simply execute the executable.
		</description>
	</enum>
	<enum name="cmd">
		<description>
			Execute in a command prompt (relevant for Windows).
		</description>
	</enum>
	<enum name="cmdenv">
		<description>
			Execute in a command prompt (relevant for Windows) and with this RapidEnv project's environment set before.
		</description>
	</enum>
</enumtype>
