<enumtype name="org.rapidbeans.rapidenv.RunMode">
	<description>Determine if to run in command line or from GUI.</description>
	<enum name="command">
		<description>run in command line</description>
	</enum>
	<enum name="batch">
		<description>run in command line without user interaction</description>
	</enum>
	<enum name="gui">
		<description>run in graphical user interface</description>
	</enum>
	<enum name="test">
		<description>run in test mode</description>
	</enum>
</enumtype>
